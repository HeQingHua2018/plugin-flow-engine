{
  "name": "plugin-flow-engine",
  "version": "1.0.0",
  "private": true,
  "description": "规则引擎库",
  "keywords": [
    "plugin",
    "flow",
    "engine"
  ],
  "homepage": "https://flow.chloehe.cn",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/HeQingHua2018/plugin-flow-engine.git"
  },
  "author": "HeQingHua",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run --scope @plugin-flow-engine/core build && lerna run --scope @plugin-flow-engine/ui && lerna run --scope @plugin-flow-engine/type build",
    "changelog": "conventional-changelog -p angular -r 0 -o ./packages/doc/docs/versions/changelog.md",
    "clean": "lerna clean -y && rimraf ./node_modules/",
    "dev": "lerna run --scope @plugin-flow-engine/core dev && lerna run --scope @plugin-flow-engine/ui && lerna run --scope @plugin-flow-engine/type dev",
    "format": "prettier --write ./**/*.{js,jsx,ts,tsx,css,less,json,md}",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx --fix --quiet ./",
    "lint:stylelint": "stylelint --fix ./**/*.less",
    "prepare": "husky",
    "publish:lerna": "lerna publish from-package --yes --access public",
    "version:lerna": "lerna version",
    "commit": "cz"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@types/lodash": "^4.17.20",
    "json-rules-engine": "^7.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^24.9.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/lint": "^4.0.0",
    "@xyflow/react": "^12.9.0",
    "antd": "^5.27.6",
    "babel-plugin-import": "^1.13.8",
    "conventional-changelog-angular": "^8.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "cz-customizable": "^7.4.0",
    "dumi": "^2.4.13",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "husky": "^8.0.1",
    "lerna": "^6.6.2",
    "less": "^3.13.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^3.0.2",
    "stylelint": "^14.9.1"
  },
  "peerDependencies": {
    "@xyflow/react": "^12",
    "antd": "^5",
    "react": "^18",
    "react-dom": "^18"
  },
  "packageManager": "pnpm@10.18.2",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
